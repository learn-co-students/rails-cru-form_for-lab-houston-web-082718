<h1>Songs#edit</h1>
<p>Find me in app/views/songs/edit.html.erb</p>

<h3>Update Song Form</h3>
<%= form_for(@song) do |f| %>
  <label>Song Name:</label><br>
  <%= f.text_field :name %><br>

  <label>Artist Name:</label><br>
  <%= f.text_field :artist_id %><br>

  <label>Genre:</label><br>
  <%= f.text_field :genre_id %><br>

  <%= f.submit %>
<% end %>
