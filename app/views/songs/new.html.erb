<h1>Songs#new</h1>
<p>Find me in app/views/songs/new.html.erb</p>

<h3>New Song Form</h3>
<%= form_for(@song) do |f| %>
  <label>Song Name:</label><br>
  <%= f.text_field :name %><br>

  <label>Artist Name:</label><br>
  <%= f.text_field :artist %><br>

  <label>Genre:</label><br>
  <%= f.text_field :genre %><br>

  <%= f.submit %>
<% end %>
